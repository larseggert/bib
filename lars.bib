@techreport{rfc6824,
	Author = {Ford, Alan and Raiciu, Costin and Handley, Mark and Bonaventure, Olivier},
	Date-Added = {2017-09-11 08:41:33 +0000},
	Date-Modified = {2017-09-11 08:49:33 +0000},
